@page "/"
@namespace Corona_Doe_UI.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Corona_Doe_UI</title>
    <base href="~/" />
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
    <link rel="stylesheet" href="lib/font-awesome/css/font-awesome.css" />
    <link href="lib/openlayers/ol.css" type="text/css" rel="stylesheet" />
    <link href="css/site.css" rel="stylesheet" />

    <!--Drop Down-->
    <link rel="stylesheet" href="_content/LoreSoft.Blazor.Controls/BlazorControls.css" />
    <script src="_content/LoreSoft.Blazor.Controls/BlazorControls.js"></script>

</head>
<body>
    <app>
        <component type="typeof(App)" render-mode="ServerPrerendered" />
    </app>

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>

    <script src ="lib/jquery/dist/jquery.min.js"></script>
    <script src="lib/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/js/Custom.js"></script>

    <script src="_framework/blazor.server.js"></script>
    <script src="lib/openlayers/ol.js"></script>
    <script type="text/javascript" src="js/Map.js"></script>
</body>
</html>
